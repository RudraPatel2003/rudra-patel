---
import type { HTMLAttributes } from "astro/types";

type Props = {
  attributes?: HTMLAttributes<"svg">;
};

const { attributes } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width="24"
  height="24"
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class="lucide lucide-arrow-left transition-transform duration-150 hover:translate-x-0.5"
  {...attributes}
>
  <path d="M19 12H5"></path>
  <path d="M12 19L5 12L12 5"></path>
</svg>
